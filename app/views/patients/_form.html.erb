<%= simple_form_for(@patient) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :ssn %>
    <%= f.input :date_of_birth %>
    <%= f.association :accession_log %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
