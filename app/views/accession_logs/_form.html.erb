<%= simple_form_for(@accession_log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date_received %>
    <%= f.input :date_received_physicians_office %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
