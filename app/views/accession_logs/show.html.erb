<%- model_class = AccessionLog -%>
<div class="page-header">
  <h1><%=t '.title', :default => AccessionLog %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:date_received) %></dt>
      <dd><pre class="prettyprint"><%= @accession_log.date_received %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:date_received_physicians_office) %></dt>
      <dd><pre class="prettyprint"><%= @accession_log.date_received_physicians_office %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              accession_logs_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_accession_log_path(@accession_log), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              accession_log_path(@accession_log),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

